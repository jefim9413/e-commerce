{
  "name": "e-commerce",
  "version": "1.0.0",
  "description": "e-commerce api",
  "main": "index.js",
  "scripts": {
    "start:dev": "tsx watch src/server.ts",
    "start": "node build/server.js",
    "build": "tsup src --out-dir build",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "test:e2e": "vitest run --dir tests/e2e --config vitest.config.ts",
    "test:e2e:watch": "vitest --dir tests/e2e --config vitest.config.ts --watch",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint . --ext .ts --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/cors": "10.1.0",
    "@fastify/jwt": "9.0.4",
    "@prisma/client": "6.4.1",
    "bcryptjs": "3.0.2",
    "dotenv": "16.4.7",
    "fastify": "5.2.1",
    "zod": "3.24.2"
  },
  "devDependencies": {
    "@rocketseat/eslint-config": "2.1.0",
    "@types/bcryptjs": "2.4.6",
    "@types/jsonwebtoken": "9.0.9",
    "@types/node": "22.13.5",
    "@types/supertest": "6.0.2",
    "prisma": "6.4.1",
    "supertest": "7.0.0",
    "eslint": "8.52.0",
    "tsup": "8.3.6",
    "tsx": "4.19.3",
    "typescript": "5.7.3",
    "vitest": "3.0.7"
  }
}
